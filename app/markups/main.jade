doctype html 
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(http-equiv="x-ua-compatible" content="ie=edge")
		link(rel="stylesheet" href="fonts/Fonts.css")
		link(rel="stylesheet" href="bower/jquery-ui/themes/flick/jquery-ui.css")
		link(rel="stylesheet" href="bower/select2/dist/css/select2.min.css")
		link(rel="stylesheet" href="css/main.css")
		title
			block title
	body
		include _common/_header					
		.wrapper.clearfix
			.bread-crumbs 
				span.bread_item Главная
				span.bread_item >
				span.bread_item_last Каталог
			h1.page-name Мобильные телефоны
			include _common/_aside	
			include _common/_section
		.wrapper.clearfix
			ul.pages
				li.pages_item 
					a.pages_link <
				li.pages_item 
					a.pages_link 1
				li.pages_item 
					a.pages_link 2
				li.pages_item 
					a.pages_link 3
				li.pages_item 
					a.pages_link >
		.wrapper
			.info
				include _common/info
		footer.footer
			include _common/footer
		script(src="bower/jquery/dist/jquery.min.js")
		script(src="bower/jquery.columnizer/src/jquery.columnizer.min.js")
		script(src="bower/select2/dist/js/select2.full.min.js")
		script(src="bower/jquery-ui/jquery-ui.min.js")
		script(src="js/main.js")			